{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.NewRelic.Logs",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Thread",
      "Serilog.Enrichers.Process"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] [{IpAddress}] {Message:lj}{NewLine}{Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "NewRelicLogs",
        "Args": {
          "applicationName": "{New Relic Application Name}",
          "licenseKey": "{New Relic License Key}",
          "endpointUrl": "https://log-api.eu.newrelic.com/log/v1"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId",
      "WithEnvironmentName",
      "WithNewRelicLogsInContext"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "DatabaseSeeder": {
    "Enabled": false,
    "SeedSampleData": false,
    "AdminEmail": "",
    "AdminPassword": "",
    "AdminFirstName": "",
    "AdminLastName": ""
  },
  "HomePage": {
    "LatestBlocks": [
      { "SectionTitle": "Latest in Personal Finance", "CategoryId": 68 },
      { "SectionTitle": "Latest in Technology", "CategoryId": 69 },
      { "SectionTitle": "Latest in Productivity", "CategoryId": 70 }
    ]
  },
  "Navbar": {
    "CategorySlugs": [ "finance", "tech", "productivity", "software", "other" ]
  }
}
